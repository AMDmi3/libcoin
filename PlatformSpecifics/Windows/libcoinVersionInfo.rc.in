            VALUE "ProductName", "libcoin"
            VALUE "ProductVersion", "${LIBCOIN_MAJOR_VERSION}, ${LIBCOIN_MINOR_VERSION}, ${LIBCOIN_PATCH_VERSION}, ${LIBCOIN_SOVERSION}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
